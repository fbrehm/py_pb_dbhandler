#: src/pb_dbhandler/handler.py:363
msgid "Destroying base database object."
msgstr "Zerstöre Datenbankobjekt."

#: src/pb_dbhandler/handler.py:389
#, python-format
msgid "Error performing .pgpass file, using empty password: %s"
msgstr "Fehler bei der Verabeitung der .pgpass-Datei, verwende leeres Passwort: %s"

#: src/pb_dbhandler/handler.py:408
msgid ""
"Trying to establish a connection while an existing database connection."
msgstr ""
"Versuche eine Datenbankverbindung einzurichten, während schon eine existiert."

#: src/pb_dbhandler/handler.py:424
msgid "Used parameters to connect to database:"
msgstr "Verwendete Parameter für die Datenbankverbindung:"

#: src/pb_dbhandler/handler.py:430
msgid "Setting DB connection to readonly."
msgstr "Setze die Datenbnk-Verbindung auf nur-lesend."

#: src/pb_dbhandler/handler.py:441
#, python-format
msgid "DB connection: %r"
msgstr "Datenbankverbindung: %r"

#: src/pb_dbhandler/handler.py:444
msgid "DB connection closed."
msgstr "Datenbankverbindung geschlossen."

#: src/pb_dbhandler/handler.py:446
msgid "DB connection opened."
msgstr "Datenbankverbindung geöffnet."

#: src/pb_dbhandler/handler.py:447
#, python-format
msgid "DB cursor: %r"
msgstr "Datenbankcursor: %r"

#: src/pb_dbhandler/handler.py:450
msgid "Not connected to database."
msgstr "Nicht mit der Datenbank verbunden."

#: src/pb_dbhandler/handler.py:456
msgid "Opening database cursor."
msgstr "Öffne Datenbankcursor."

#: src/pb_dbhandler/handler.py:475
msgid "Closing database cursor."
msgstr "Schließe Datenbankcursor."

#: src/pb_dbhandler/handler.py:495
msgid "Closing the database connection."
msgstr "Schließe Datenbankverbindung."

#: src/pb_dbhandler/handler.py:498
msgid "Destroying db connection object."
msgstr "Zerstöre Datenbankverbindungsobjekt."

#: src/pb_dbhandler/helper.py:47
msgid "The port number of a PostgreSQL database must be greater than zero."
msgstr "Die Portnummer einer PostgreSQL-Datenbank muss größer als Null sein."

#: src/pb_dbhandler/helper.py:48
#, python-format
msgid "The port number of a PostgreSQL database must be less or equal %d."
msgstr "Die Portnummer einer PostgreSQL-Datenbank muss kleiner oder gleich %d sein."

#: src/pb_dbhandler/helper.py:83
msgid "Database options"
msgstr "Datenbankoptinen"

#: src/pb_dbhandler/helper.py:85
#, python-format
msgid "The host of the PostgreSQL database (Default: %r)."
msgstr "Der Hostname der PostgreSQL-Datenbank (Vorgabe: %r)."

#: src/pb_dbhandler/helper.py:95
#, python-format
msgid "The TCP port of PostgreSQL database on the database machine (Default: %d)."
msgstr ""
"Die Portnummer der PostgreSQL-Datenbank auf dem Datenbankserver (Vorgabe: %d)."

#: src/pb_dbhandler/helper.py:106
msgid "The database schema (database) used on DB host."
msgstr "Das Datenbankschema (die Datenbank) auf dem Datenbankserver."

#: src/pb_dbhandler/helper.py:108
#, python-format
msgid "The database schema (database) used on DB host (Default: %r)."
msgstr "Das Datenbankschema (die Datenbank) auf dem Datenbankserver (Vorgabe: %r)."

#: src/pb_dbhandler/helper.py:118
msgid "The database user using for connecting to DB."
msgstr "Der Datenbanknutzer, der für die Verbindung zur Datenbank verwendet wird."

#: src/pb_dbhandler/helper.py:120
#, python-format
msgid "The database user using for connecting to DB (Default: %r)."
msgstr ""
"Der Datenbanknutzer, der für die Verbindung zur Datenbank verwendet wird (Vorgabe: %r)."

#: src/pb_dbhandler/helper.py:130
msgid "The password of the database user connecting to DB."
msgstr "Das Paßwort des Datenbanknutzers."

#: src/pb_dbhandler/helper.py:131
msgid "The usage of this parameter is not recommended."
msgstr "Die Verwendung dieser Option wird nicht empfohlen."

#: src/pb_dbhandler/helper.py:132
msgid "Better use an appropriate entry in the $HOME/.pgpass file."
msgstr "Verwenden Sie besser einen entspechenden Eintrag in der $HOME/.pgpass Datei."

#: src/pb_dbhandler/pgpass.py:50
#, python-format
msgid "Environment variable %r not set."
msgstr "Umgebungsvariable %r nicht gesetzt."

#: src/pb_dbhandler/pgpass.py:81
#, python-format
msgid "Pgpassfile %r doesn't exists."
msgstr "Pgpass-Datei %r existiert nicht."

#: src/pb_dbhandler/pgpass.py:105
#, python-format
msgid "Pgpassfile %r is not readable."
msgstr "Pgpass-Datei %r ist nicht lesbar."

#: src/pb_dbhandler/pgpass.py:440
#, python-format
msgid "Others have permissions on %r."
msgstr "Andere (others) haben Rechte auf %r."

#: src/pb_dbhandler/pgpass.py:442
#, python-format
msgid "Group has permissions on %r."
msgstr "Die Gruppe hat Rechte auf %r."

#: src/pb_dbhandler/pgpass.py:444
#, python-format
msgid "Group and others have permissions on %r."
msgstr "Die Gruppe und andere (others) haben Rechte auf %r."

#: src/pb_dbhandler/pgpass.py:470
#, python-format
msgid "No valid content in %r found."
msgstr "Ungültigen Inhalt in %r gefunden."

#: src/pb_dbhandler/pgpass.py:493
#, python-format
msgid "Got fields: %r"
msgstr "Erhaltene Felder: %r"

#: src/pb_dbhandler/pgpass.py:495
#, python-format
msgid ""
"Invalid entry %(entry)r in %(file)r line %(rownum)d found, only %(nr)d "
"fields found instead of 5."
msgstr ""
"Ungültigen Eintrag %(entry)r in %(file)r, Zeile %(rownum)d, gefunden, "
"nur %(nr)d Felder anstelle von fünf Feldern gefunden."

#: src/pb_dbhandler/pgpass.py:507
#, python-format
msgid "Invalid port %(port)r in %(file)r line %(rownum)d found."
msgstr "Ungültigen Port %(port)r in %(file)r, Zeile %(rownum)d, gefunden."

#: src/pb_dbhandler/pgpass.py:562
#, python-format
msgid ""
"Found entries in %(file)r:\n"
"%(list)s"
msgstr ""
"Gefundene Einträge in %(file)r:\n"
"%(list)s"

#: src/pb_dbhandler/pgpass.py:573
#, python-format
msgid ""
"Password found for host %(host)r, port %(port)d, database %(db)r and user "
"%(usr)r."
msgstr ""
"Passwort für Host %(host)r, Port %(port)d, Datenbank %(db)r "
"und Nutzer %(usr)r gefunden."

#: src/pb_dbhandler/pgpass.py:575
#, python-format
msgid ""
"No password found for host %(host)r, port %(port)d, database %(db)r and user "
"%(usr)r."
msgstr ""
"Kein Passwort für Host %(host)r, Port %(port)d, Datenbank %(db)r "
"und Nutzer %(usr)r gefunden."

#: src/pb_dbhandler/translate.py:40
#, python-format
msgid "Basedir: %r"
msgstr "Basis-Verzeichnis: %r"

#: src/pb_dbhandler/translate.py:41
#, python-format
msgid "Found .mo-file: %r"
msgstr "Gefundene .mo-Datei: %r"

